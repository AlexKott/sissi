{
  "name": "sissi",
  "description": "The only thing you'll need for creating websites with sissi",
  "version": "0.5.2",
  "author": "Alex Kott @AlexKott",
  "bin": {
    "sissi": "./bin/run"
  },
  "bugs": "https://github.com/square-a/sissi/issues",
  "dependencies": {
    "fs-extra": "3.0.1",
    "sissi-moves": "^0.5.0",
    "sissi-packs": "^0.2.3",
    "sissi-says": "^0.3.0",
    "sissi-snaps": "^1.3.1",
    "yeoman-environment": "^2.2.0",
    "yeoman-generator": "^2.0.5"
  },
  "devDependencies": {
    "@babel/polyfill": "^7.0.0-rc.2",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib"
  ],
  "homepage": "https://github.com/square-a/sissi",
  "keywords": [
    "sissi",
    "react",
    "static-site-generator"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "square-a/sissi",
  "scripts": {
    "start": "./node_modules/.bin/babel ./src/index.js -o ./lib/index.js --watch",
    "prepare": "rm -rf ./lib && mkdir ./lib && ./node_modules/.bin/babel ./src/index.js -o ./lib/index.js"
  }
}
